sage.success('修改成功，下次登录生效！')\n            this.loading = false\n          }).catch(() => {\n            this.loading = false\n          })\n        } else {\n          console.log('error submit!!')\n          return false\n        }\n      })\n    },\n    onCancel() {\n      this.$message({\n        message: 'cancel!',\n        type: 'warning'\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.line{\n  text-align: center;\n}\n</style>\n\n"],"sourceRoot":"src/views/setting/mima"}]}