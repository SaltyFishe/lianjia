A;AAWA,IAAA,SAXA,uBAWA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,KAAA,EAAA,EADA;AAEA,QAAA,GAAA,EAAA,EAFA;AAGA,QAAA,YAAA,EAAA,EAHA;AAIA,QAAA,SAAA,EAAA,EAJA;AAKA,QAAA,SAAA,EAAA,EALA;AAMA,QAAA,QAAA,EAAA,EANA;AAOA,QAAA,QAAA,EAAA;AAPA,OAAA;AASA,KArBA;AAsBA,IAAA,SAtBA,uBAsBA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA;AACA,MAAA,YAAA,CAAA,KAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,KAAA;AACA,OAHA;AAIA;AA5BA;AAhCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <el-form ref=\"form\" :inline=\"true\" :model=\"form\" label-width=\"80px\">\r\n      <el-form-item label=\"关键词\">\r\n        <el-input v-model=\"form.title\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"房屋用途\">\r\n        <el-select v-model=\"form.use\" placeholder=\"请选择房屋用途\">\r\n          <el-option label=\"普通住宅\" value=\"普通住宅\"></el-option>\r\n          <el-option label=\"商住两用\" value=\"商住两用\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"装修情况\">\r\n        <el-select v-model=\"form.decorate\" placeholder=\"请选择装修情况\">\r\n          <el-option label=\"精装\" value=\"精装\"></el-option>\r\n          <el-option label=\"简装\" value=\"简装\"></el-option>\r\n          <el-option label=\"毛坯\" value=\"毛坯\"></el-option>\r\n          <el-option label=\"其他\" value=\"其他\"></el-option>\r\n        </el-select>\r\n      </el-form-item>\r\n      <el-form-item label=\"面积范围\">\r\n        <el-input v-model=\"form.min_size\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\":\">\r\n        <el-input v-model=\"form.max_size\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"fetchData\">搜索</el-button>\r\n        <el-button @click=\"clearForm\">清空</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <el-table\r\n      v-loading=\"listLoading\"\r\n      :data=\"houseList.slice((currentPage-1)*pagesize,currentPage*pagesize)\"\r\n      element-loading-text=\"Loading\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n    >\r\n      <el-table-column align=\"center\" label=\"ID\" width=\"95\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.$index + 1 }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"标题\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-link target=\"_blank\" type=\"primary\" :href=\"scope.row.url\">{{ scope.row.title }}</el-link>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"地区\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.area }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"小区\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.community }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"价格（万元）\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.price }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"房型\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.layout }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"大小（平米）\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.size }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"朝向\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.toward }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"装修\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.decorate }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"楼层\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.layer }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"挂牌时间\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.year }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"交易权属\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.type }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"房屋用途\">\r\n        <template slot-scope=\"scope\">\r\n          {{ scope.row.use }}\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"currentPage\"\r\n      :page-sizes=\"[5, 10, 20, 40]\"\r\n      :page-size=\"pagesize\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"houseList.length\">  /*每页显示多少条数据*/\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getHouseList} from '@/api/table'\r\n\r\nexport default {\r\n  filters: {\r\n    statusFilter(status) {\r\n      const statusMap = {\r\n        published: 'success',\r\n        draft: 'gray',\r\n        deleted: 'danger'\r\n      }\r\n      return statusMap[status]\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      listLoading: true,\r\n      form: {\r\n        gearbox: '',\r\n        title: '',\r\n        age: '',\r\n        gearbox_type: '',\r\n        max_price: '',\r\n        min_price: '',\r\n        min_size: '',\r\n        max_size: ''\r\n      },\r\n      currentPage: 1,\r\n      pagesize: 10,\r\n      houseList: []\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchData()\r\n  },\r\n  methods: {\r\n    // 初始页currentPage、初始每页数据数pagesize和数据data\r\n    handleSizeChange: function(size) {\r\n      this.pagesize = size\r\n    },\r\n    handleCurrentChange: function(currentPage) {\r\n      this.currentPage = currentPage\r\n    },\r\n    handleUserList() {\r\n\r\n    },\r\n    clearForm() {\r\n      this.form = {\r\n        title: '',\r\n        age: '',\r\n        gearbox_type: '',\r\n        max_price: '',\r\n        min_price: '',\r\n        max_size: '',\r\n        min_size: ''\r\n      }\r\n    },\r\n    fetchData() {\r\n      this.listLoading = true\r\n      getHouseList(this.form).then(response => {\r\n        this.houseList = response.datas\r\n        this.listLoading = false\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/source"}]}